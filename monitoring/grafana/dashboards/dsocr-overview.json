{
  "title": "DSOCR Overview",
  "uid": "dsocr-overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Queue Size",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [{"expr": "queue_size"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "stat",
      "title": "Running Workers",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [{"expr": "running_workers"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "stat",
      "title": "GPU Free GB",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "targets": [{"expr": "gpu_memory_free_gb"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}}
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [{"expr": "sum by (path,method) (rate(http_requests_total[1m]))"}]
    },
    {
      "type": "timeseries",
      "title": "Request Latency (p95)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le,path,method) (rate(http_request_duration_seconds_bucket[5m])))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Tasks Succeeded/Failed",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {"expr": "rate(tasks_succeeded_total[1m])", "legendFormat": "succeeded"},
        {"expr": "rate(tasks_failed_total[1m])", "legendFormat": "failed"}
      ]
    }
  ]
}

